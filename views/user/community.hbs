<main class='jumbotron'>
<h1><strong>Comunidad</strong></h1>

<div class="container justify-content-center">
    <div class="row">
    {{#each users}}
    <div class="col-md-4">

    <div class="card artist-container shadow-sm" style="width: 18rem;">
    <a href="/community/{{_id}}"><img src="{{avatar}}" class="card-img-top" alt="profileImg"></a>
    <div class="card-body">
        <h5 class="card-title">{{title}}</h5>

        <a href="/community/{{_id}}">{{username}}</a>
    </div>
    </div>
</div>

{{/each}}

</main>